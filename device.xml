<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="origin" fetch="git@github.com:alfadiyaefx" />

  <!-- Include vendor manifest as a project -->
  <project name="vendor-manifest" path="vendor/manifest" remote="origin" revision="main" />

  <!-- Declare it as a submanifest -->
  <submanifest
      name="vendor-manifest"
      remote="origin"
      project="vendor-manifest"
      manifest-name="vendor.xml"
      revision="main" />
</manifest>
